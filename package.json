{
  "name": "maintainpro-cmms",
  "version": "1.0.0",
  "description": "Enterprise Maintenance Management System",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "seed": "tsx scripts/database/seed.ts",
    "build": "vite build && npx tsc --noEmit",
    "start": "NODE_ENV=production tsx server/index.ts",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "clean": "rm -rf dist node_modules/.vite coverage",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config tests/config/vitest.config.unit.ts",
    "test:integration": "vitest run --config tests/config/vitest.config.integration.ts",
    "test:e2e": "playwright test --config tests/config/playwright.config.ts",
    "test:e2e:ui": "playwright test --ui --ui-host=0.0.0.0 --config tests/config/playwright.config.ts",
    "test:e2e:headed": "playwright test --headed --config tests/config/playwright.config.ts",
    "test:e2e:debug": "playwright test --debug --config tests/config/playwright.config.ts",
    "test:e2e:report": "playwright show-report test-results/reports/playwright-report",
    "test:performance": "echo 'Install k6 globally to run performance tests'",
    "test:accessibility": "vitest run --config tests/config/vitest.config.accessibility.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:mutation": "stryker run",
    "test:mutation:debug": "stryker run --logLevel debug",
    "test:smoke": "npm run test:e2e -- --grep '@smoke'",
    "test:smoke:staging": "ENVIRONMENT=staging npm run test:smoke",
    "test:smoke:production": "ENVIRONMENT=production npm run test:smoke",
    "quality": "npm run lint:check && npm run format:check && npm run type-check && npm run test:run",
    "domains:list": "./scripts/development/domain-management.sh list",
    "domains:update-main": "./scripts/development/domain-management.sh update-main",
    "domains:update-stable": "./scripts/development/domain-management.sh update-stable",
    "domains:update-all": "./scripts/development/domain-management.sh update-all",
    "domains:verify": "./scripts/development/domain-management.sh verify",
    "domains:setup": "./scripts/development/domain-management.sh setup",
    "deploy:railway": "./deploy.sh",
    "railway:logs": "railway logs",
    "railway:status": "railway status",
    "migration:phase1": "./scripts/testing/test-migration-phase1.sh",
    "migration:test-api": "./scripts/testing/test-migration-api.sh",
    "migration:deploy": "./scripts/deployment/production-deploy.sh",
    "migration:status": "echo 'Phase 1: âœ… COMPLETE - PostgreSQL Storage Active'",
    "migration:report": "./scripts/migration/migration-final-report.sh",
    "production:start": "./scripts/deployment/production-start.sh",
    "project:add-issues": "node scripts/project-management/add-issues-to-project.js",
    "project:test": "node scripts/project-management/test-project-management.js",
    "prepare": "is-ci || husky install",
    "lint-staged": "lint-staged"
  },
