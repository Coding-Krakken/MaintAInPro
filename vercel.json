{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",

  "routes": [
    {
      "src": "/sw.js",
      "dest": "/api/sw.js"
    },
    {
      "src": "/manifest.json",
      "dest": "/api/manifest.json"
    },
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/equipment",
      "dest": "/api/equipment"
    },
    {
      "src": "/api/work-orders",
      "dest": "/api/work-orders"
    },
    {
      "src": "/api/dashboard/stats",
      "dest": "/api/dashboard/stats"
    },
    {
      "src": "/api/notifications",
      "dest": "/api/notifications"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],

  "env": {
    "NODE_ENV": "production"
  },

  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
